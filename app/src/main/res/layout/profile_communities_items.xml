<?xml version="1.0" encoding="utf-8"?>
<appliedlife.pvtltd.SHEROES.views.cutomeviews.RippleViewLinear xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:id="@+id/profile_community_container"
    android:layout_height="wrap_content">

    <LinearLayout
        android:background="@drawable/background_community_profile"
        android:layout_margin="@dimen/dp_size_4"
        android:orientation="vertical"
        android:layout_width="@dimen/dp_size_160"
        android:layout_height="@dimen/dp_size_150">

    <appliedlife.pvtltd.SHEROES.views.cutomeviews.CircleImageView
        android:id="@+id/community_icon"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp_size_16"
        android:layout_width="@dimen/dp_size_65"
        android:layout_height="@dimen/dp_size_65" />

    <TextView
        android:maxLines="@integer/int_value_for_line_2"
        android:ellipsize="end"
        android:layout_gravity="center_horizontal"
        android:id="@+id/community_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dp_size_8"
        android:textSize="14sp"
        android:fontFamily="sans-serif"
        android:textStyle="normal"
        android:textColor="#878787"
        android:lineSpacingExtra="6sp"
        android:gravity="center_horizontal"
        tools:text="Parenting by Babygogo Babygogo"/>

    </LinearLayout>
</appliedlife.pvtltd.SHEROES.views.cutomeviews.RippleViewLinear>