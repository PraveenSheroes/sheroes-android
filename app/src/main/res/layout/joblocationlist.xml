<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_marginTop="@dimen/dp_size_22"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tv_location_nm"
        android:layout_marginTop="@dimen/dp_size_10"
        android:layout_width="wrap_content"
        android:textColor="@color/job_filter_type_color"
        android:layout_height="wrap_content"
        android:textSize="@dimen/sp_size_13"
        android:fontFamily="@string/ID_ROBOTO_REGULAR"
        android:text="@string/ID_COMMUNITY_TYPE"/>
    <TextView
        android:id="@+id/tv_check"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/job_filter_desc_color"
        android:fontFamily="@string/ID_ROBOTO_REGULAR"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:textSize="@dimen/sp_size_13"
        android:layout_marginTop="@dimen/dp_size_10"/>

</RelativeLayout>