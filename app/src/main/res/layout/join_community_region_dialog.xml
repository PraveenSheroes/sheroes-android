<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:vernacular="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <appliedlife.pvtltd.SHEROES.vernacular.FontTextView
        style="@style/TextView14spBold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="@string/ID_JOIN_REGION_TXT"
        android:textStyle="bold"
        vernacular:customTextFontFamily="@string/bold_font" />

    <appliedlife.pvtltd.SHEROES.vernacular.FontTextView
        android:id="@+id/tv_higly_intreseted"
        style="@style/TextView14spRegular"
        android:layout_width="@dimen/dp_size_282"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp_size_21"
        android:background="@drawable/join_intrest_text_shap"
        android:gravity="center"
        android:paddingBottom="@dimen/dp_size_5"
        android:paddingLeft="@dimen/dp_size_20"
        android:paddingRight="@dimen/dp_size_20"
        android:paddingTop="@dimen/dp_size_5"
        android:text="@string/ID_COMMUNITY_JOIN_REGION1"
        android:textColor="@color/blue"
        vernacular:customTextFontFamily="@string/regular_font" />

    <appliedlife.pvtltd.SHEROES.vernacular.FontTextView
        android:id="@+id/tv_already_member"
        style="@style/TextView14spRegular"
        android:layout_width="@dimen/dp_size_282"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="@dimen/dp_size_50"
        android:layout_marginTop="@dimen/dp_size_20"
        android:background="@drawable/join_intrest_text_shap"
        android:gravity="center"
        android:paddingBottom="@dimen/dp_size_5"
        android:paddingTop="@dimen/dp_size_5"
        android:text="@string/ID_COMMUNITY_JOIN_REGION2"
        android:textColor="@color/blue"
        vernacular:customTextFontFamily="@string/regular_font" />
</LinearLayout>
