<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
      xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_size_50"
        android:layout_marginTop="@dimen/dp_size_10"
        android:layout_marginBottom="@dimen/dp_size_10"
        android:background="@color/white">

     <TextView
            android:id="@+id/tv_other_view"
            android:layout_width="wrap_content"
            android:layout_marginLeft="@dimen/dp_size_16"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:text=""
            android:layout_height="wrap_content"
            android:fontFamily="@string/ID_ROBOTO_REGULAR"
            android:textColor="@color/text_color_setting_preference_basicdetails"
            android:textSize="@dimen/sp_size_13" />

        <TextView
            android:id="@+id/tv_edit_other_textline"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:textSize="@dimen/dp_size_17"
            android:layout_centerVertical="true"
            android:background="@drawable/other_count_backgroung_shap"
            android:fontFamily="@string/ID_ROBOTO_REGULAR"
            android:textColor="@color/white"
            android:text="1"
            android:layout_marginRight="@dimen/dp_size_16"



            />



</RelativeLayout>
